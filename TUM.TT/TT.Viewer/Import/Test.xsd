<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="DataSetTT" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="Rank">
    <xs:complexType>
      <xs:attribute name="Position" type="xs:string" />
      <xs:attribute name="Date" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Match">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="FirstPlayer" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Rank" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
            <xs:attribute name="Name" type="xs:string" />
            <xs:attribute name="Nationality" type="xs:string" />
            <xs:attribute name="Spielsystem" type="xs:string" />
            <xs:attribute name="Händigkeit" type="xs:string" />
            <xs:attribute name="Griffhaltung" type="xs:string" />
            <xs:attribute name="Material" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element name="SecondPlayer" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Rank" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
            <xs:attribute name="Name" type="xs:string" />
            <xs:attribute name="Nationality" type="xs:string" />
            <xs:attribute name="Spielsystem" type="xs:string" />
            <xs:attribute name="Händigkeit" type="xs:string" />
            <xs:attribute name="Griffhaltung" type="xs:string" />
            <xs:attribute name="Material" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Rallies" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Rally" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CurrentRallyScore" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="First" type="xs:string" />
                        <xs:attribute name="Second" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="CurrentSetScore" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:attribute name="First" type="xs:string" />
                        <xs:attribute name="Second" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Schlag" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Schlagtechnik" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                              <xs:attribute name="Art" type="xs:string" />
                              <xs:attribute name="Option" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Spin" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                              <xs:attribute name="US" type="xs:string" />
                              <xs:attribute name="ÜS" type="xs:string" />
                              <xs:attribute name="SL" type="xs:string" />
                              <xs:attribute name="SR" type="xs:string" />
                              <xs:attribute name="No" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Platzierung" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                              <xs:attribute name="WX" type="xs:string" />
                              <xs:attribute name="WY" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Nummer" type="xs:string" />
                        <xs:attribute name="Spieler" type="xs:string" />
                        <xs:attribute name="Schlägerseite" type="xs:string" />
                        <xs:attribute name="Aufschlagart" type="xs:string" />
                        <xs:attribute name="Qualität" type="xs:string" />
                        <xs:attribute name="Spielerposition" type="xs:string" />
                        <xs:attribute name="Besonderes" type="xs:string" />
                        <xs:attribute name="Verlauf" type="xs:string" />
                        <xs:attribute name="Umlaufen" type="xs:string" />
                        <xs:attribute name="Balltreffpunkt" type="xs:string" />
                        <xs:attribute name="Aggressivität" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="Nummer" type="xs:string" />
                  <xs:attribute name="Winner" type="xs:string" />
                  <xs:attribute name="Length" type="xs:string" />
                  <xs:attribute name="Server" type="xs:string" />
                  <xs:attribute name="Anfang" type="xs:string" />
                  <xs:attribute name="Ende" type="xs:string" />
                  <xs:attribute name="Kommentar" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Tournament" type="xs:string" />
      <xs:attribute name="Round" type="xs:string" />
      <xs:attribute name="Mode" type="xs:string" />
      <xs:attribute name="DateTime" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="DataSetTT" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element ref="Rank" />
        <xs:element ref="Match" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>
