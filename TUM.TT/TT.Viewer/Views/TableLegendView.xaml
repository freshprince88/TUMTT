<local:TableView x:Class="TT.Viewer.Views.TableLegendView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TT.Viewer.Views"
             xmlns:cal="http://www.caliburnproject.org"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="200"
             Strokes="{Binding Strokes}">
    <Grid Height="300" Width="200" Background="White" Focusable="true"> <!--cal:Message.Attach="[Event MouseLeave] = [Action MouseLeft()]"-->
        <Border Name="TableBorder" BorderThickness="1" BorderBrush="Black" Panel.ZIndex="-99"/>
        <Grid Name="InnerFieldGrid" Margin="30,10,30,10"/>
        <Grid Name="InnerFieldBehindGrid" Margin="15,10,15,10"/>
    </Grid>
</local:TableView>
