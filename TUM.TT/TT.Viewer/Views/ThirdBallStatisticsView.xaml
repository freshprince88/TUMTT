                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                            </Grid>
                            <Grid Grid.Row="2" Grid.Column="1" Margin="2 1 0 1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <ToggleButton x:Name="PlacementForehandHalfLongTotalButton" Tag="r => r.Schlag[2].IsMidLeft()" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0"  FontSize="40"   >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandHalfLongTotalButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandHalfLongPointsWonButton" Tag="r => (r.Schlag[2].IsMidLeft()) &amp;&amp; r.Schlag[2].Spieler == r.Winner" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Padding="2 0 2 0"  FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandHalfLongPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandHalfLongDirectPointsWonButton" Tag="r => r.Schlag[2].IsMidLeft() &amp;&amp; r.Schlag[2].Spieler == r.Winner &amp;&amp; r.Length &lt; 5" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="1" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch"  Padding="1 0 1 0"     Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandHalfLongDirectPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandHalfLongPointsLostButton" Tag="r => r.Schlag[2].IsMidLeft() &amp;&amp; r.Schlag[2].Spieler != r.Winner" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="2" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch" Padding="1 0 1 0" FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandHalfLongPointsLostButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                            </Grid>
                            <Grid Grid.Row="3" Grid.Column="1" Margin="2 0 0 0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
