<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
             xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:conv="clr-namespace:TT.Lib.Converters;assembly=TT.Lib"
             xmlns:cal="http://www.caliburnproject.org"
             xmlns:local="clr-namespace:TT.Viewer.Views" x:Class="TT.Viewer.Views.ThirdBallStatisticsView"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="300">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Icons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/FlatSlider.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Themes/SplitButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="Container">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Foreground="{DynamicResource AccentColorBrush}"><Run Text="3. Schlag"/></TextBlock>
        <Line X1="0" X2="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Y1="0" Y2="0" Grid.Row="1" Stroke="{DynamicResource AccentColorBrush}" StrokeDashArray="6,2"  Margin="0 5 0 0"/>
        <ContentControl x:Name="BasicFilterStatisticsView" Grid.Row="2" />
        <Line X1="0" X2="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Y1="0" Y2="0" Grid.Row="3" Stroke="{DynamicResource AccentColorBrush}"   Margin="0 5 0 0"/>
        <!--<ContentControl x:Name="PlacementStatisticsView" Grid.Row="4" />-->
        <Grid x:Name="StatisticsContainer" Grid.Row="4">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0" x:Name="BasisInformation" Margin="0 0 10 0">
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MinWidth="70" />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />

                </Grid.ColumnDefinitions>
                <Label Grid.Row="0" Grid.Column="0" FontSize="10" Margin="5 0 5 0" VerticalAlignment="Center" HorizontalAlignment="Left" Content="Anzahl:"/>
                <Label Grid.Row="1" Grid.Column="0" FontSize="10" Margin="5 0 5 0" VerticalAlignment="Center" HorizontalAlignment="Left" Content="Punkt:"/>
                <Label x:Name="BasisInformationLabelPlayer1"  Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2"  VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding Path=Player1}"/>
                <Label x:Name="BasisInformationLabelPlayer2" Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding Path=Player2}"/>
                <ToggleButton x:Name="TotalThirdBallsCount" Tag="r => r.Length >=3" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="4"   Style="{DynamicResource MetroAccentFlatToggle}" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" FontWeight="Bold">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="Click">
                            <cal:ActionMessage MethodName="StatButtonClick">
                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                <cal:Parameter Value="{Binding ElementName=TotalThirdBallsCount, Path=Name}" />
                            </cal:ActionMessage>
                            <cal:ActionMessage MethodName="SelectBasisInformation">
                                <cal:Parameter Value="$source" />
                            </cal:ActionMessage>
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                    <ToggleButton.Content>
                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag" />
                        </MultiBinding>
                    </ToggleButton.Content>
                </ToggleButton>
                <ToggleButton x:Name="TotalThirdBallsCountPointPlayer1" Tag="r => r.Length >=3 &amp;&amp; r.Winner=='First'" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"   Style="{DynamicResource MetroAccentFlatToggle}" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" FontWeight="Bold" >
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="Click">
                            <cal:ActionMessage MethodName="StatButtonClick">
                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                <cal:Parameter Value="{Binding ElementName=TotalThirdBallsCountPointPlayer1, Path=Name}" />
                            </cal:ActionMessage>
                            <cal:ActionMessage MethodName="SelectBasisInformation">
                                <cal:Parameter Value="$source" />
                            </cal:ActionMessage>
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                    <ToggleButton.Content>
                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                        </MultiBinding>
                    </ToggleButton.Content>
                </ToggleButton>
                <ToggleButton x:Name="TotalThirdBallsCountPointPlayer2" Tag="r => r.Length >=3 &amp;&amp; r.Winner=='Second'" Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="2" Style="{DynamicResource MetroAccentFlatToggle}" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" FontWeight="Bold" >
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="Click">
                            <cal:ActionMessage MethodName="StatButtonClick">
                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                <cal:Parameter Value="{Binding ElementName=TotalThirdBallsCountPointPlayer2, Path=Name}" />
                            </cal:ActionMessage>
                            <cal:ActionMessage MethodName="SelectBasisInformation">
                                <cal:Parameter Value="$source" />
                            </cal:ActionMessage>
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                    <ToggleButton.Content>
                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                        </MultiBinding>
                    </ToggleButton.Content>
                </ToggleButton>
            </Grid>
            <Line X1="0" X2="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}" Y1="0" Y2="0" Grid.Row="1" Stroke="{DynamicResource AccentColorBrush}" StrokeDashArray="6,2"  Margin="0 5 0 0"/>
            <Grid x:Name="PlacementGrid" Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Expander  Grid.Row="0" BorderThickness="0" Background="Transparent"  Controls:ControlsHelper.ContentCharacterCasing="Normal" IsExpanded="False">
                    <Expander.Header>
                        <Label HorizontalAlignment="Center" Content="Platzierung"/>
                    </Expander.Header>
                    <Viewbox Grid.Row="1">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" MinHeight="50"/>
                                <RowDefinition Height="auto" MinHeight="50" />
                                <RowDefinition Height="auto" MinHeight="50"/>
                                <RowDefinition Height="auto" MinHeight="50"/>
                                <RowDefinition Height="auto" MinHeight="50"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="20"/>
                                <ColumnDefinition Width="auto" MinWidth="60"/>
                                <ColumnDefinition Width="auto" MinWidth="60"/>
                                <ColumnDefinition Width="auto" MinWidth="60"/>
                                <ColumnDefinition Width="auto" MinWidth="60"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Row="0" Grid.RowSpan="5" Grid.Column="0" Grid.ColumnSpan="5" Background="{DynamicResource GrayBrush5}"/>
                            <Border Grid.Row="1" Grid.Column="1" Grid.RowSpan="3" Grid.ColumnSpan="3" BorderBrush="White" BorderThickness="2 2 2 0" Background="{DynamicResource AccentColorBrush}"/>
                            <Border Grid.Row="4" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="3" BorderBrush="White" BorderThickness="2 2 2 0" Background="{DynamicResource AccentColorBrush}"/>
                            <Border Grid.Row="0" Grid.RowSpan="4" Grid.Column="2"  BorderBrush="Black" BorderThickness="1 0 1 0" />
                            <Border Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="4" Margin="2 0 0 0" BorderBrush="Black" BorderThickness="0 1 0 1" />

                            <Label Grid.Row="0" Grid.Column="4"  HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"  Content="∑"/>
                            <Label Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"  Content="l"/>
                            <Label Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="h"/>
                            <Label Grid.Row="3" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="k"/>
                            <Label Grid.Row="4" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="F"/>
                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <ToggleButton x:Name="PlacementForehandAllTotalButton" Tag="r => r.Schlag[2].IsTopLeft() || r.Schlag[2].IsMidLeft() || r.Schlag[2].IsBotLeft()" Style="{DynamicResource MetroAccentFlatToggle}" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0"  FontSize="40">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandAllTotalButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandAllPointsWonButton" Tag="r => (r.Schlag[2].IsTopLeft() || r.Schlag[2].IsMidLeft() || r.Schlag[2].IsBotLeft()) &amp;&amp; r.Schlag[2].Spieler == r.Winner"  Style="{DynamicResource MetroAccentFlatToggle}" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Padding="2 0 2 0"  FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandAllPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandAllDirectPointsWonButton" Tag="r => (r.Schlag[2].IsTopLeft() || r.Schlag[2].IsMidLeft() || r.Schlag[2].IsBotLeft()) &amp;&amp; r.Schlag[2].Spieler == r.Winner &amp;&amp; r.Length &lt; 5" Style="{DynamicResource MetroAccentFlatToggle}" Grid.Row="1" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch"  Padding="1 0 1 0"     Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandAllDirectPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandAllPointsLostButton" Tag="r => (r.Schlag[2].IsTopLeft() || r.Schlag[2].IsMidLeft() || r.Schlag[2].IsBotLeft()) &amp;&amp; r.Schlag[2].Spieler != r.Winner" Style="{DynamicResource MetroAccentFlatToggle}" Grid.Row="2" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch" Padding="1 0 1 0" FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandAllPointsLostButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                            </Grid>
                            <Grid Grid.Row="1" Grid.Column="1" Margin="2 2 0 0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <ToggleButton x:Name="PlacementForehandLongTotalButton" Tag="r => r.Schlag[2].IsTopLeft()" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0"  FontSize="40"   >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandLongTotalButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandLongPointsWonButton" Tag="r => r.Schlag[2].IsTopLeft() &amp;&amp; r.Schlag[2].Spieler == r.Winner" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Padding="2 0 2 0"  FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandLongPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandLongDirectPointsWonButton" Tag="r => r.Schlag[2].IsTopLeft() &amp;&amp; r.Schlag[2].Spieler == r.Winner &amp;&amp; r.Length &lt; 5" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="1" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch"  Padding="1 0 1 0"     Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandLongDirectPointsWonButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
                                        <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="OneWay">
                                            <Binding Path="BasicFilterStatisticsView.SelectedRallies" />
                                            <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                                        </MultiBinding>
                                    </ToggleButton.Content>
                                </ToggleButton>
                                <ToggleButton x:Name="PlacementForehandLongPointsLostButton" Tag="r => r.Schlag[2].IsTopLeft() &amp;&amp; r.Schlag[2].Spieler != r.Winner" Style="{DynamicResource MetroAccentFlatGrayCheckedToggle}" Grid.Row="2" Grid.Column="1"  VerticalAlignment="Stretch"  HorizontalAlignment="stretch" Padding="1 0 1 0" FontWeight="Bold"    Visibility="Visible">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Click">
                                            <cal:ActionMessage MethodName="StatButtonClick">
                                                <cal:Parameter Value="{Binding ElementName=StatisticsContainer}" />
                                                <cal:Parameter Value="{Binding ElementName=PlacementForehandLongPointsLostButton, Path=Name}" />
                                            </cal:ActionMessage>
                                            <cal:ActionMessage MethodName="SelectPlacement">
                                                <cal:Parameter Value="$source" />
                                            </cal:ActionMessage>
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <ToggleButton.Content>
