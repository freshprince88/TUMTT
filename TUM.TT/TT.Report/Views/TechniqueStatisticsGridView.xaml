<UserControl x:Class="TT.Report.Views.TechniqueStatisticsGridView" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:conv="clr-namespace:TT.Converters;assembly=TT.Converters"
             xmlns:prop="clr-namespace:TT.Report.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="414" 
             d:DesignWidth="300">

    <UserControl.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="GrayBrush5">#FFB9B9B9</SolidColorBrush>
            <SolidColorBrush x:Key="GrayBrush2">#FF7F7F7F</SolidColorBrush>
            <prop:Resources x:Key="LocalizedStrings"/>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid ShowGridLines="False" x:Name="MainGrid" Width="300" Height="414">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="30"/>
            <ColumnDefinition Width="*" MinWidth="50"/>
            <ColumnDefinition Width="*" MinWidth="50"/>
            <ColumnDefinition Width="*" MinWidth="50"/>
            <ColumnDefinition Width="*" MinWidth="50"/>
            <ColumnDefinition Width="0"/>
        </Grid.ColumnDefinitions>

        <Border Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="4" Background="{StaticResource GrayBrush5}"/>
        <Border Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="3" Background="{StaticResource GrayBrush5}"/>
        <Border Grid.Row="8" Grid.Column="4" Background="{StaticResource GrayBrush2}"/>
        <Border Grid.Row="6" Grid.Column="4" Grid.RowSpan="2" Background="{StaticResource GrayBrush5}"/>

        <Border Grid.Row="0" Grid.Column="4" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 0 0" />

        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 0 0 1" />
        <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 1 0 1"/>
        <Border Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 1 0 1"/>
        <Border Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 2 0 0"/>
        <Border Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 0 0 1" />
        <Border Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 1 0 1"/>
        <Border Grid.Row="7" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 1 0 1"/>
        <Border Grid.Row="8" Grid.Column="0" Grid.ColumnSpan="6" BorderBrush="Black" BorderThickness="0 2 0 0"/>

        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="0 0 1 0" />
        <Border Grid.Row="0" Grid.Column="1" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="0" Grid.Column="2" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="0" Grid.Column="3" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="0" Grid.Column="4" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 0 0"/>

        <Border Grid.Row="5" Grid.Column="0" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="0 0 1 0" />
        <Border Grid.Row="5" Grid.Column="1" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="5" Grid.Column="2" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="5" Grid.Column="3" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="1 0 1 0"/>
        <Border Grid.Row="5" Grid.Column="4" Grid.RowSpan="4" BorderBrush="Black" BorderThickness="2 0 0 0" />






        <Label Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=technique_label_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=side_forehand_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=side_backhand_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="3" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="∑"/>
        <Label Grid.Row="5" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=technique_label_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="6" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=side_forehand_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="7" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="{Binding Path=side_backhand_short, Source={StaticResource LocalizedStrings}}"/>
        <Label Grid.Row="8" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Content="∑"/>


        <Grid x:Name="FlipTotalGrid" Tag="Flip" Margin="1 2 1 0" >
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <Grid.Column>
                <MultiBinding Converter="{conv:TechniqueCountToRankingColumnConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                    <Binding Path="SelectedRallies" />
                    <Binding ElementName="AllFlipTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushShortTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushHalfLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllChopTotalButton" Path="Tag"/>
					<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
					<Binding Path="Player" />
					<Binding Path="StrokeNumber" />
                </MultiBinding>
            </Grid.Column>
            <Grid.Row>
                <MultiBinding Converter="{conv:TechniqueCountToRankingRowConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                    <Binding Path="SelectedRallies" />
                    <Binding ElementName="AllFlipTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushShortTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushHalfLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllChopTotalButton" Path="Tag"/>
					<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
					<Binding Path="Player" />
					<Binding Path="StrokeNumber" />
                </MultiBinding>
            </Grid.Row>
            <Label x:Name="AllFlipTotalButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip'" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0" FontSize="40" HorizontalAlignment="Center">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="AllFlipPointsWonButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="2 0 2 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="AllFlipDirectPointsWonButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner &amp;&amp; r.Length &lt; 4" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="AllFlipPointsLostButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player != r.Winner" Grid.Row="2" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
        </Grid>
        <Label Grid.Column="{Binding ElementName=FlipTotalGrid, Path=Column}" Tag="Label" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="10">
            <Grid.Row>
                <MultiBinding Converter="{conv:LabelBackhandForehandRowConverter}" Mode="OneWay">
                    <Binding ElementName="FlipTotalGrid" Path="Row"/>
                    <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                </MultiBinding>
            </Grid.Row>
            <TextBlock TextAlignment="Center" Text="{Binding Path=section_technique_flip, Source={StaticResource LocalizedStrings}}" />
        </Label>
        <Grid Grid.Column="{Binding ElementName=FlipTotalGrid, Path=Column}" Tag="Forehand" Margin="1 1 1 1" >
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <Grid.Row>
                <MultiBinding Converter="{conv:LabelBackhandForehandRowConverter}" Mode="TwoWay">
                    <Binding ElementName="FlipTotalGrid" Path="Row"/>
                    <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                </MultiBinding>
            </Grid.Row>
            <Label x:Name="ForehandFlipTotalButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Forehand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip'" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0" FontSize="40" HorizontalAlignment="Center">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="ForehandFlipPointsWonButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Forehand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="2 0 2 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="ForehandFlipDirectPointsWonButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Forehand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner &amp;&amp; r.Length &lt; 4" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="ForehandFlipPointsLostButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Forehand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player != r.Winner" Grid.Row="2" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
        </Grid>
        <Grid  Grid.Column="{Binding ElementName=FlipTotalGrid, Path=Column}" Tag="Backhand" Margin="1 1 1 1">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <Grid.Row>
                <MultiBinding Converter="{conv:LabelBackhandForehandRowConverter}" Mode="TwoWay">
                    <Binding ElementName="FlipTotalGrid" Path="Row"/>
                    <Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
                </MultiBinding>
            </Grid.Row>
            <Label x:Name="BackhandFlipTotalButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Backhand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip'" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0" FontSize="40" HorizontalAlignment="Center">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="BackhandFlipPointsWonButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Backhand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="2 0 2 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="BackhandFlipDirectPointsWonButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Backhand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner &amp;&amp; r.Length &lt; 4" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="BackhandFlipPointsLostButton" Tag="r => $$Player$$ r.Strokes[$$StrokeNumber$$].Side == 'Backhand' &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Flip' &amp;&amp; r.Strokes[$$StrokeNumber$$].Player != r.Winner" Grid.Row="2" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="1 0 1 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
        </Grid>



        <Grid x:Name="PushShortTotalGrid" Tag="PushShort" Margin="1 2 1 0" >
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <Grid.Column>
                <MultiBinding Converter="{conv:TechniqueCountToRankingColumnConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                    <Binding Path="SelectedRallies" />
                    <Binding ElementName="AllFlipTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushShortTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushHalfLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllChopTotalButton" Path="Tag"/>
					<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
					<Binding Path="Player" />
					<Binding Path="StrokeNumber" />
                </MultiBinding>
            </Grid.Column>
            <Grid.Row>
                <MultiBinding Converter="{conv:TechniqueCountToRankingRowConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                    <Binding Path="SelectedRallies" />
                    <Binding ElementName="AllFlipTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushShortTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushHalfLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllPushLongTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllTopspinParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockDiagonalTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockParallelTotalButton" Path="Tag"/>
                    <Binding ElementName="AllBlockMiddleTotalButton" Path="Tag"/>
                    <Binding ElementName="AllChopTotalButton" Path="Tag"/>
					<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
					<Binding Path="Player" />
					<Binding Path="StrokeNumber" />
                </MultiBinding>
            </Grid.Row>
            <Label x:Name="AllPushShortTotalButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Push' &amp;&amp; r.Strokes[$$StrokeNumber$$].IsShort()" Grid.Row="0" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="1" Padding="2 0 2 0" FontSize="40" HorizontalAlignment="Center">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
						<Binding RelativeSource="{RelativeSource Self}" Path="Tag"/>
						<Binding Path="Player" />
						<Binding Path="StrokeNumber" />
					</MultiBinding>
                </Label.Content>
            </Label>
            <Label x:Name="AllPushShortPointsWonButton" Tag="r => $$Player$$ (r.Strokes[$$StrokeNumber$$].Side == 'Forehand' || r.Strokes[$$StrokeNumber$$].Side == 'Backhand') &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique != null &amp;&amp; r.Strokes[$$StrokeNumber$$].Stroketechnique.Type == 'Push' &amp;&amp; r.Strokes[$$StrokeNumber$$].IsShort() &amp;&amp; r.Strokes[$$StrokeNumber$$].Player == r.Winner" Grid.Row="0" Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Center" Padding="2 0 2 0" FontWeight="Bold" Visibility="Visible">
                <Label.Content>
                    <MultiBinding Converter="{conv:RallyExpressionToCountConverter}" Mode="TwoWay" ConverterParameter="r.Strokes[$$StrokeNumber$$].Player == '$$PlayerString$$' &amp;&amp;">
                        <Binding Path="SelectedRallies" />
